{
  "name": "auth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon app.js --ext js,jsx --ignore ./sessions",
    "db-init-seed-user": "npx sequelize-cli seed:generate --name demo-user",
    "db-init-seed-token": "npx sequelize-cli seed:generate --name demo-token",
    "db-seed": "npx sequelize-cli db:seed:all",
    "db-unseed": "npx sequelize db:seed:undo:all",
    "db-create": "npx sequelize db:create",
    "db-drop": "npx sequelize db:drop",
    "db-migrate": "npx sequelize db:migrate:undo:all && npx sequelize db:migrate",
    "db-user": "npx sequelize-cli model:generate --name User --attributes firstName:string,lastName:string,birthDay:date,email:string,password:string,role:string,isActivated:boolean,activationString:string,officeId:integer,companyId:integer",
    "db-token": "npx sequelize-cli model:generate --name Token --attributes refreshToken:string,userId:integer"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "sequelize-cli": "^6.6.1"
  }
}
